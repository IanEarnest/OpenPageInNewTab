{
   "manifest_version": 3,
   "name": "Open page in new tab",
   "version": "1.0",
   
   "description": "Add a context menu item to load a new tab of the current page URL.",
   "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"	  
   },
	"background": {
		"service_worker": "background.js"
	},
   "permissions": [ 
		"contextMenus"
	]
}